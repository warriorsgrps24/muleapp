
pipeline
{
agent any
stages{
stage('Build Apllication'){
steps{
echo 'application is in building phase'
bat 'mvn clean install'
}
}


stage('Test){
steps{
echo 'application is in testing phase'
bat 'mvn test'
}
}
stage('Deploy to Cloudhub'){
environment {
                ANYPOINT_CREDENTIALS = credentials(platform.credentials)
            }
steps{
bat 'mvn clean package deploy -DmuleDeploy Dmule=4.4.0 DUSERNAME=${ANYPOINT_CREDENTIALS_USR}'
}
}
}
}